{
  "name": "slack",
  "version": "1.2.2",
  "private": true,
  "description": "Notify Slack of GitHub Release",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "coverage": "jest --coverage",
    "all": "npm run build && npm run format && npm run lint && npm run pack && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openhousepvt/slack.git"
  },
  "keywords": [
    "actions",
    "slack",
    "notify"
  ],
  "author": "arch10",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^4.0.0",
    "@octokit/webhooks": "^7.24.3",
    "@slack/webhook": "^5.0.4",
    "flow-bin": "^0.138.0",
    "graphql": "^15.5.1",
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^14.17.6",
    "@typescript-eslint/parser": "^4.28.5",
    "@zeit/ncc": "^0.22.3",
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.1.5",
    "eslint-plugin-jest": "^24.4.0",
    "jest": "^24.9.0",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.1",
    "prettier": "^2.3.2",
    "ts-jest": "^24.3.0",
    "typescript": "^4.3.5"
  }
}
